*{
    padding: 0;
    margin: 0;
    font-family: "微软雅黑";
    
}
ul,li{
    list-style: none;
}
a{
    text-decoration: none;
}
.clearfix::after{
    content: ".";
    height: 0;
    visibility:hidden;
    display:block;
    clear:both;
}
/* 顶部 */
.header{
    min-width: 1200px;
    margin: 0 auto;
    font-size: 12px;
    color: #666;
}
/* 登录条 */
.header .loginbar{
    height: 30px;
    width: 100%;
    background: #f7f7f7;
    border-bottom: 1px solid #ededed;
}
.header .loginbar .center{
    width: 1200px;
    height: 22px;
    
    line-height: 22px;
    margin: 6px auto 0;
}
.header .loginbar-left{
    float: left;
    margin-top:5px;
}
.header .loginbar-left div{
    margin:1px 5px 0 0;
}
.header .loginbar-right{
    float: right;
}
.header .loginbar-left div,.header .loginbar-left p{
    float: left;
    
}

/* a标签去样式 */
.header a:link{

    color: #666;
}
.header a:visited{

    color: #666;
}
.header a:hover{

    color: #666;
}
.header a:active{

    color: #666;
}
/* 登陆a标签样式 */

.header .login a:link{
    color:blue;
}

.header .list li{
    float: left;
}
.header .hello{
    width: 132px;
    height: 27px;
    padding-left: 12px;
}
.header .login{
    width: 106px;
    height: 27px;
    padding: 0 12px;
}
.header .me{
    width: 64px;
    height: 27px;
    padding-left:12px;
    

}
.header .list img{
    vertical-align: middle;
}
.header .app{
    width: 64px;
    height: 27px;
    padding-left:26px;
    background: url(../images/mwb09/header_1407.png) 10px 3px no-repeat;
    cursor: pointer;
}
.header .all{
    width: 88px;
    height: 27px;
    padding-left:32px;
 
    position: relative;
}
.header .all span{
    display: inline-block;
    width:17px;
    height: 17px;
    position: absolute;
    left: 5px;
    top: 4px;
    background: url(../images/mwb09/header_1407.png) -320px 0 no-repeat;
}
.header .map{
    width: 48px;
    height: 27px;
    padding-left:12px;
}
.header .help{
    width: 64px;
    height: 27px;
    padding-left:12px;
}
.header .number{
    width: 61px;
    height: 27px;
    padding-left: 12px;
    
}
.header .number span{
    display: inline-block;
    width: 60px;
    height: 18px;
    line-height: 22px;

    background: url(../images/mwb09/header_1407.png) -259px -24px no-repeat;
}
.header .interval{
    width: 5px;
    /* margin-right: 5px; */
}
/* 登陆条结束 */
/* 搜索条 */
.header .search{
    width: 1200px;
    margin: 0 auto;
    padding: 12px 0 7px 0;
}
.header .search .logo{
    width: 300px;
    float: left;
}
.header .search .search-box{
    width: 550px;
    float: left;
    margin-top: 10px;
}
.header .search .search-box input{
    width: 450px;
    height: 30px;
    border: 3px solid #c6181c;
    padding: 0 5px;
    font-size: 15px;
    line-height: 30px;
    color:#999999;
}
.header .search .search-box button{
    width: 73px;
    height: 36px;
    /* background: #c6181c; */
    border: 1px solid #c6181c;
    margin-left: -5px;
    vertical-align: bottom;
    background: url(../images/mwb09/header_1407.png) -370px -46px no-repeat;
    cursor: pointer;
}
.header .search .search-box p{
    margin-top: 5px;
}
.header .search .search-box p a{
    margin: 0 5px;
}
.header .search .search-box a.color:link{
    color:red;
}
.header .search .shopping-trolley{
    position: relative;
    width: 128px;
    height: 36px;
    border: 1px solid #bdbdbd;
    float: right;
    background: url(../images/mwb09/header_1407.png) 0px -85px no-repeat;
    padding-left: 32px;
    line-height: 36px;
    margin-top: 10px;
}
.header .search .shopping-trolley .number{
    width: 5px;
    height: 5px;
    position: absolute;
    right: 50px;
    top: 0;
    color: #900;
    font-size: 16px;
}
/* 搜索条结束 */
/* 菜单条 */
.header .menu{
    width: 100%;
    height: 40px;
    background-color: #a71b1b; 
    color: #fff;
}
.header .menu .center{
    width:1200px;
    margin: 0 auto;
}
.header .menu .classification{
    width: 190px;
    height: 40px;
    line-height: 30px;
    font-size: 15px;
    background: #b82323;
    float: left
}
.header .menu .classification a{
    width: 190px;
    height: 40px;
}
.header .menu .classification .size{
    display: inline-block;
    width: 37px;
    height: 40px;
    background: url(../images/mwb09/menu.png) 0 0 no-repeat;
}
.header .menu .classification span{
    vertical-align: middle;
    color: #fff;
}
.header .menu .classification span img.arrows{
    vertical-align: middle;
    margin-left: 30px;
}
.header .menu .list{
    float: left;
}
.header .menu .list li{
    float: left;
    width: 100px;
    height: 39px;
    font-size: 15px;
    text-align: center;
    line-height: 39px;
    /* color: #fff; */
}
.header .menu .list a:link{
    color:#fff;
}
/* 搜索条结束 */
