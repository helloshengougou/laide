/* 推荐 */
.recommend{
    width: 1200px;
    margin: 0 auto;
}
.recommend .recommend-top{
    margin-top: 2px;
    height: 45px;
}
.recommend .recommend-bottom{
    /* border: 1px solid red; */
    margin-top:20px;
    margin-left: 20px;
    cursor: pointer; 
}
/* 推荐结束 */
/* 主要内容 */
.main{
    width: 100%;
    background: url(../images/mwb09/bg.jpg) repeat;
    /* height: 5000px; */
    padding-top: 20px;
}
.main .ewm{
    width:950px;
    height: 165px;
    margin: 0 auto ;
}
.main .modules1{
    width: 980px;
    margin: 0 auto;
    padding-bottom: 25px;
}

.main .modules1 .boutique{
    margin-top: 20px;
    padding-top: 25px;
    background: url(../images/mwb09/trapezoid.png) center top no-repeat;
    height: 60px;
    
}
.main .modules1 .boutique div{
    height: 60px;
    background-color: #ff7c11;
    font-size: 35px;
    line-height: 56px;
    color: #fff;
    font-weight: bolder;
    text-align: center;
}
.main .modules1 .commodity{
    width: 950px;
    background: #ff7c11;
    padding: 0 20px 0 10px;
    /* height: 100px; */
}
.main .modules1 ul{
    width: 950px;
    padding-bottom:10px; 
    margin-left: 4px;
}
.main .modules1 ul li{
    width: 230px;
    height: 376px;
    background: #fff;
    margin-bottom: 10px;
    margin-left: 7px;
    cursor: pointer; 
    float: left;
}
.main .modules1 .commodity-img img{
    width: 230px;
    height: 215px;
}
.main .modules1 .products-introduction{
    width: 210px;
    margin: 10px auto;
}
.main .modules1 .products-introduction h3{
    font-size: 14px;
    color: #666;
    margin-bottom: 10px ;
    font-weight: 400;
}
.main .modules1 .products-introduction .spell-group-price{
    background: url(../images/mwb09/btns.png) -213px -48px no-repeat; 
    width: 52px;
    height: 20px;
    color: #fff;
    padding-left: 5px;
    font-size: 14px; 
}
.main .modules1 .products-introduction .price{
    margin-top: 5px;
    height: 30px;
    line-height: 30px;
}
.main .modules1 .products-introduction .new-price{
    font-size: 26px;
    color: #ff004e;
}
.main .modules1 .products-introduction .original-price{
    color: #888;
    font-size: 18px;
    text-decoration: line-through;
}
.main .modules1 .products-introduction .immediately{
    width: 100%;
    height: 36px;
    background-color: #fd004d;
    color: #fff;
    border-radius: 5px;
    line-height: 36px;
    text-align: center;
    
}
.main .modules1 .more{
    width: 100%;
    /* height: 40px; */
    /* border: 1px solid red; */
    padding-top: 10px;
}
.main .modules1 .more .more-position{
    float: right;
    width: 234px;
    height: 40px; 
    background: url(../images/mwb09/more.png) 0px 0px no-repeat; 
    cursor: pointer; 
}
/* 主要内容结束 */
/* 团购活动 */
.activity{
    padding: 25px 0;
    background:#109996;
}
.activity .activity-img{
    text-align: center;
    height: 60px;
}
.activity .activity-rules{
    width: 920px;
    margin: 10px auto 0;
}
.activity .activity-rules{
    font-size: 16px;
    line-height: 25px;
    color: #fff;
    letter-spacing: 1px;
}
/* 团购活动结束 */
/* 底部 */
.footer{
    width: 1200px;
    margin: 20px auto;
}
.footer .footer-banner{
    height: 56px;
    width: 100%;
    padding: 20px 0;
    border-bottom: 1px solid #ddd;
    background: url(../images/mwb09/footer-banner.png) 0px -122px no-repeat;
}
.footer .footer-banner a{
    float: left;
    height: 52px;
    cursor: pointer;
    /* line-height: 52px; */
}
.footer .footer-banner a.banner1{
    width: 80px;
    margin-left: 30px;
}
.footer .footer-banner a.banner2{
    width: 120px;
    margin-left: 30px;
}
.footer .footer-banner a.banner3{
    width: 200px;
    margin-left: 95px;
}
.footer .footer-banner a.banner4{
    width: 200px;
    margin-left: 95px;
}
/* 购物助手链接 */
.footer .footer-helper{
    width: 1200px;
    padding:15px 0 20px; 
}
.footer a:link{
    text-decoration:none;
    color: #888;
}
.footer a:visited{
    text-decoration:none;
    color: #888;
}
.footer a:hover{
    text-decoration:none;
    color: #888;
}
.footer a:active{
    text-decoration:none;
    color: #888;
}
.footer .footer-helper ul li{
    width: 120px;
    /* height: 182px; */
    font-size: 12px;
    float: left;
    padding-right: 92px;
}
.footer .footer-helper ul li h4{
    color: #666;
    line-height: 22px;
    border-bottom: dotted 1px #ccc;
    margin: 0 0 5px;
}
.footer .footer-helper ul li .href-box{
    width: 120px;
}
.footer .footer-helper ul li .href-box a{
    width: 120px;
    float: left;
    line-height: 22px;
}
.footer .footer-helper ul li.footer-left{
    padding-left: 20px;
}
.footer .footer-helper ul li.footer-right{
    padding: 0;
}
/* 购物助手链接结束 */
.footer .footer-license{
    font-size: 12px;
    color:#999;
    padding: 15px 0 5px;
    border-top: solid 1px #ddd;
}
.footer .copyright{
    line-height: 20px;
    color: #999;
    font-size: 12px;
}
.footer .footer-icon{
    margin-top: 15px;
    height: 34px;
    /* border: 1px solid red; */
    background: url(../images/mwb09/footer-banner.png) 0px 0px no-repeat;
}
.footer .footer-icon a{
    float: left;
    width: 100px;
    height: 34px;
    margin-right: 10px;
}
/* 底部结束 */
