.search .search-box{
    width: 1200px;
    margin: 0 auto;
    height: 40px;
}
.search .fix-logo{
    background-image: url(../images/index/zjl_icon_002.png);
    width: 54px;
    height: 40px;
    float: left;
}
.search .search-list{
    position: absolute;
    top: 36px;
    left: 0;
    padding: 0;
    width:450px;
    border: solid 1px #dfdfdf;
    background: #fff;
    z-index: 5;
    box-shadow: 1px 1px 3px #dfdfdf;
}
.search .search-list li{
    padding: 3px 8px 4px 8px;
    line-height: 26px;
    cursor: pointer;
    margin: 0;
    display: block;
    clear: both;
    overflow: hidden;
    color: #666;
}
.search .search-list span{
    color: #222;
}
.search .search-list li:hover{
    background-color: #f3f3f3;
}
.search .search-list li:hover .fl{
    color: #c51010;
}

/* ======================搜索 结束 */

/*===================导航*/
.nav{
    width: 100%;
    min-width: 1200px;
    background-color: #a71b1b;
}
.nav .nav-content{
	width: 1200px;
	height: 40px;
	margin:0 auto;
    position: relative;
    z-index: 2;
}
.nav .nav-left,.nav .nav-right{
    float: left;
}
.nav .all-class{
    width: 190px;
    height: 40px;
    background: url(../images/index/sprite_category.png) 0px -16px no-repeat;
}
.nav .all-class >a{
    font-size: 14px;
    padding: 2px;
    margin-left: 42px;
    line-height: 39px;
    color: #fff;
    background-color: #b82424;
    font-weight: bold;
}
.nav .nav-right li{
    float: left;

}
.nav .nav-right li a{
    width: 100px;
    height: 40px;
    line-height: 40px;
    display: block;
    text-align: center;
    color: #fff;
    font-size: 15px;
    font-weight: bolder;
    text-decoration: none;
}
.nav .nav-right li a:hover{
    background-color: #6c0709
}
.nav .nav-left a{
    text-decoration: none;
}
.nav .nav-left .all-class:hover{
    background: url(../images/index/sprite_category.png) 0px -76px no-repeat;
}
.nav .nav-left .all-class:hover >a{
    background: #c52e15;
}
.nav .item{
    width: 190px;
    height: 470px;
    background-color: rgba(255,255,255,0.9);
    position: absolute;
    left: 0;
    top: 40px;
    display: none;
}
.nav .item>ul>li{
    width: 170px;
    height: 36px;
    line-height: 36px;
    padding: 0 5px 0 15px;
    font-size: 13px;
}
.nav .item i{
    float: left;
    width: 16px;
    height: 16px;
    margin:10px 10px 0 0;
    transform: rotate(0deg);
    transition:all .5s;
}
.nav .feature i{
    background: url(../images/index/sprite_category.png) -144px 0px no-repeat
}
.nav .beauty i{
    background: url(../images/index/sprite_category.png) -48px 0px no-repeat
}
.nav .home i{
    background: url(../images/index/sprite_category.png) -192px 0px no-repeat
}
.nav .kitchen i{
    background: url(../images/index/sprite_category.png) -176px 0px no-repeat
}
.nav .household i{
    background: url(../images/index/sprite_category.png) -32px 0px no-repeat
}
.nav .education i{
    background: url(../images/index/sprite_category.png) -128px 0px no-repeat
}
.nav .bags i{
    background: url(../images/index/sprite_category.png) -80px 0px no-repeat
}
.nav .phone i{
    background: url(../images/index/sprite_category.png) -0px 0px no-repeat
}
.nav .clothes i{
    background: url(../images/index/sprite_category.png) -64px 0px no-repeat
}
.nav .movement i{
    background: url(../images/index/sprite_category.png) -112px 0px no-repeat
}
.nav .computer i{
    background: url(../images/index/sprite_category.png) -16px 0px no-repeat
}
.nav .car i{
    background: url(../images/index/sprite_category.png) -272px -16px no-repeat
}
.nav .card i{
    background: url(../images/index/sprite_category.png) -256px 0px no-repeat
}
.nav .item li:hover i{
    transform: rotate(360deg);
    transition: all .5s;
}
.nav .item >ul>li>a:hover{
    text-decoration: underline;
    color: #c51010;
}
.nav .item >ul>li:hover {
    box-shadow: 1px 1px 3px #888;
    
    background-color: #fff;

}
.nav .item-box{
    width: 1010px;
    height: 470px;
    background-color: #fff;
    position: absolute;
    top: 0;
    left: 190px;
    display: none;
}
.nav .item-list-box{
    width: 700px;
    float: left;
}
.nav .item-class{
    float: left;
}
.nav .item-list-box ul{
    overflow: hidden;
}
.nav .item-list-box li{
    float: left;
    line-height: 22px;
    font-size: 12px;
    margin-right: 20px;
}
.nav .item-ad{
    width: 330px;
}
.nav .item>ul>li:hover .item-box {
    display: block;
}
.nav .item-class a{
    color: #333;
    font-size: 14px;
    font-weight: bold;
}
.nav .item-class{
   width: 100px;
   padding: 0 20px 0 10px;
   line-height: 22px;
   font-size: 12px;
}
.nav .item-list-box div{
    width: 100%;
    border-bottom: 1px solid #ebebeb;
    padding: 8px 0;
}
.nav .item-list-box li a:hover{
    color: #c51010;
    text-decoration: underline;
}
.nav .item-ad{
    width: 260px;
    height: 470px;
    padding: 10px 25px 0;
    background-color: #fff;
    float: left;
}
.nav .item-ad-top div{
    padding: 5px 10px;
    float: left;
}
.nav .item-ad-top img{
    width: 110px;
    height: 52px;
}

.nav .item-list-box-bottom{
    position: absolute;
    bottom: 0;
    left: 20px;
}
.nav .item-list-box-bottom a{
    float: left;
    margin: 0 30px 45px 0; 
}
.nav .last-list{
    width: 600px;
    float: left;
}
.nav .last{
    width: 380px;
    height: 400px;
    padding: 70px 0 0;
    background-color: #fff;
    float: left;
}
/*===================导航 结束*/

