*{
    color: #666;
    font-family: "微软雅黑";
}
a{
    display: inline-block;
}
/*页头*/
.header{
    width: 100%;
    min-width: 1200px;
}
.header .head-top{
    background: #f7f7f7;
    height: 29px;
    text-align: center;
    padding-top: 6px;
    border-bottom: 1px solid #ededed;
}
.head-top .layout{
    width: 1200px;
    margin: auto;
    line-height: 22px;
}
.head-top .layout .fl a{
    background: url(../images/specialSale-img/home.png)no-repeat left 0;
    padding-left: 20px;
}

/*头部列表*/

.head-top .layout .fr li{
    float: left;
    padding: 0 12px;
    color: #666;
}
.head-top .arr,.phone-icn,.ify-list,.tlp{
    width: 14px;
    height: 16px;
    display: inline-block;
}
/*背景箭头*/
.head-top .arr{
    background: url(../images/specialSale-img/header_1407.png)no-repeat -430px 4px;
}
/*登陆*/
.head-top .login a{
    color: #06c;
}
/*我的莱德*/
.head-top .my{
    position: relative;
}
.head-top .my:hover{
    background: #fff;
    border: 1px solid #d9d9d9;
}
.head-top .my-list{
    position: absolute;
    top: 22px;
    left: -1px;
    width: 72px;
    padding-right: 14px;
    background: #fff;
    border: 1px solid #d9d9d9;
    border-top: #fff;
}
.head-top .my-list a{
    line-height: 28px;
}
/*手机莱德*/
.header .layout .phone:hover{
    background: #fff;
    border: 1px solid #d9d9d9;
    border-bottom: #fff;
}
.header .phone{
    position: relative;
}
.header .app-down{
    position: absolute;
    left: 50%;
    margin-left: -90px;
    width: 180px;
    height: 158px;
    padding: 10px 0;
    background: #fff;
    border: 1px solid #ededed;
    border-radius: 8px;
}
.header .layout .app-down img{
    vertical-align: middle;
}

.head-top .fr .phone .phone-icn{
    margin-top: 3px;
    background: url(../images/specialSale-img/header_1407.png)no-repeat;
}
/*全部商品分类*/
.head-top .ify{
    height: 29px;
    /*position: relative;*/
}
.head-top .ify:hover{
    background: #fff;
}
.head-top .ify-list{
    margin-top: 3px;
    margin-right: 3px;
    background:url(../images/specialSale-img/header_1407.png)no-repeat -320px 0;
}
.header .all-class-list-box{
    width: 100%;
    min-width: 1200px;
    background-color: #fff;
    position: absolute;
    top: 40px;
    left: 0;
    display: none;
    padding-bottom: 15px;
    border-bottom: 1px solid #ededed;
    z-index: 1;
}
.header .all-class-list{
    width: 1200px;
    margin: 0 auto;
}
.header .all-class-list li{
    width: 240px;
    height: 70px;
    margin: 15px 35px 0 0;
    float: left;
}
.header .all-class-list h4 {
    border-bottom: 1px solid #d9d9d9;
    font-weight: bold;
}
.header .all-class-list h4 a{
    font-size: 14px;
    line-height:26px;
}
.header .all-class-list p a{
    line-height:22px;
    margin-right: 10px;
    float: left;
}
.header .more{
    float: right
}
.header .all-class-list a{
    text-decoration: none;
}
.header .all-class-list p a:hover{
    color: #c52d15;
}
.header .all-class-list h4:hover a{
    color: #c52d15;
}

/*帮助中心*/
.header .help-center{
    position: relative;
}
.header .help-center:hover{
    background: #fff;
    border: 1px solid #d9d9d9;
    border-bottom: none;
}
.header .help-center .help-down{
    position: absolute;
    top: 22px;
    left: -1px;
    border: 1px solid #d9d9d9;
    border-top: none;
    background: #fff;
    width: 60px;
    padding-right: 26px;
}
.header .help-center .help-down a{
    line-height: 28px;
    padding-left: 12px;
}



/*电话*/
.head-top .tlp{
    margin-top: 3px;
    background:url(../images/specialSale-img/header_1407.png)no-repeat -260px -26px;
}
.head-top .tlpnum{
    padding-left: 4px;
    color: #c51010;
    font-weight: 500;
    letter-spacing: 2px;
}
.head-top .my,.phone,.ify,.map,.help-center,.telephone{
    background: url(../images/specialSale-img/header_1407.png)no-repeat -150px 5px;
}


/*logo */
.header .head-logo{
    width: 1200px;
    height: 80px;
    margin: auto;
    padding-bottom: 7px;
}
.header .head-logo .logo{
    width: 75px;
    height: 75px;
    background: #bdbdbd;
}

/*搜索*/
.head-logo .head-search{
    margin-left: 217px;
    margin-top: 10px;
}
.head-logo .search-box{
    width: 533px;
}
.head-logo .search{
    width: 444px;
    height: 30px;
    font-size: 14px;
    border: 3px solid #c7181d;
    padding: 0 5px;
}
.head-logo .search-btn{
    width: 73px;    
    height: 36px;
    background: url(../images/specialSale-img/header_1407.png)no-repeat -370px -46px;
    cursor:pointer
}
/*热词*/
.head-logo .hotkeys{
    margin-top: 9px;
}
.head-logo .hotkeys a {
    margin-right: 5px;
}
.head-logo{
    padding-top: 12px;
    padding-bottom: 7px;
}

/*头部购物车*/
.head-logo .head-shopcar{
    margin-top: 10px;
    width: 158px;
    height: 34px;
    border: 1px solid #bdbdbd;
    background:url(../images/specialSale-img/header_1407.png)no-repeat 0 -86px;
}
.head-logo .shopcar{
    display:block;
    padding-left: 31px;
    line-height: 31px;
    cursor: pointer;
}
.head-logo .head-shopcar .shopcar-num{
    display: inline-block;
    color: #900;
    margin-left: 15px;
    width: 58px;
    height: 30px;
    font-size: 18px;
    text-align: center;
    cursor: pointer;
}

/*头部导航*/
.head-nav{
    min-width: 1200px;
    background: #a71b1b;
    height: 39px;
    border-top: 1px solid #7a090b;
}
.head-nav .category{
    width: 1200px;
    margin: auto;
    position: relative;
}
/*.head-nav .all-category{
    position: relative;
}*/
.head-nav .category-item{
    width: 190px;
    height:40px;
    margin-top: -1px;
    background: url(../images/specialSale-img/sprite_category.png)no-repeat 0 -16px;
}
.head-nav .kind{
    position: absolute;
    width: 190px;
}
.head-nav .kind-icon-bg{
    display: inline-block;
    width: 16px;
    height: 16px;
    margin-right: 10px;
}
.head-nav .all-cata{
    color: #333;
    font-size: 13px;
    line-height: 25px;
    padding: 6px 5px 6px 13px;
}
.head-nav .kind-details{
    position: absolute;
    top: 0px;
    left: 190px;
    width: 1010px;
    background: #fff;
    /*font-size: 14px;*/
}
.head-nav .cata-head{
    font-size: 15px;
    border-bottom: 1px solid #ebebeb;
    margin: 0 15px;
    line-height: 37px;
}
.head-nav .cata-head li{
    width: 120px;
    color: #333;
    font-weight: 600;
    text-align: center;
    cursor: pointer;
}
.head-nav .kind-list{
    width: 110px;
    padding: 6px 10px;
    color: #000;
    font-size: 14px;
    font-weight: bold;
    line-height: 22px;
}
.head-nav .kind-list a{
    font-size: 14px;
    text-decoration: none;
    color: #333;
    margin-left: 10px;
}
.head-nav .kind-cata{
    /*width: 710px;*/
    /*margin-top: 10px;*/
}
.head-nav .cata{
    margin-top: 6px;
}
.head-nav .kind-sum{
    width: 580px;
    padding: 6px 0px;
    border-bottom: 1px solid #ebebeb;
}
.head-nav .kind-sum a{
    margin-right: 20px;
    line-height: 22px;
}
.head-nav .kind-logo{
/*    position: absolute;
    left: 710px;*/
    width: 300px;
    margin-top: 5px;
}
.head-nav .kind-logo img{
    width: 102px;
    height: 48px;
    margin-left: 35px;
    margin-top:5px;
}
.head-nav .kind-logo .little-banner{
    width: 260px;
    height: 118px;
}



.head-nav .web-item{
    height: 39px;
}
.head-nav .web-item li{
    float: left;
    width: 100px;
    text-align: center;
    line-height: 39px;
}
/*.head-nav .web-item li:hover{
    background: #6c0709;
}*/
.head-nav .web-item li a{
    width: 100px;
    color: #fff;
    font-size: 15px;
}
.head-nav .web-item li a:hover{
    text-decoration:none;
    background: #6c0709;
}