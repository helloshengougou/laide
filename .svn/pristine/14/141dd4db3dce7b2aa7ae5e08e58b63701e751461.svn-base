/* ======================头部 */

.header{
    width: 100%;
    min-width: 1200px;
    background-color: #f7f7f7;
    border-bottom: 1px solid #ededed;
    position: relative;
    color: #666;
    z-index: 6;

}
.header a{
    color: #666;
}
.header-content{
    width: 1200px;
    height: 29px;
    margin: 0 auto;
    padding-top:10px;
}
.header-left{
    float: left;
}
.header-right{
    float: right;
}
.header-right >ul>li{
    float: left;
}
.header .js_down-app,.header .js_myaccount,.header .site-map-box,.header .js_help-center,.header .call-box {
    position: relative; 
}

.header-home{
    padding: 3px 20px;
    background:url(../images/index/ico_return-home.png) 0 0 no-repeat;
}
.header .phone{
    padding-left: 15px;
    background:url(../images/index/header_1407.png) 0 0 no-repeat;
}
.header .ico-all-class{
    padding-left: 20px;
    background:url(../images/index/header_1407.png) -322px 0 no-repeat;
}
.header .ico-call{
    padding-left: 20px;
    background:url(../images/index/call.png) 0 0 no-repeat;
}
.header .line::before{
    content: '';
    border-left: 1px solid #ededed;
}
.login{
    padding: 0 10px;
}
.header .myaccount,.header .phone-box,.header .all-class,.header .site-map,.header .help-center,.header .call{
    padding: 0 10px;
    border: 1px solid transparent;
    background-color: #f7f7f7;
    position: absolute;
    top: 0px;
}

.header .js_myaccount,.header .js_help-center,.header .site-map-box,.header .call-box{
    width: 81.5px;
}

.header .myaccount,.header .help-center{
    position: absolute;
}
.header .myaccount-list, .header .help-center-list{
    background-color: #fff;
    position: absolute;
    top: 22px;
    border: 1px solid #ededed;
    border-top: 1px solid #fff;
    display: none;
}
.header .myaccount-list a,.header .help-center-list a{
    display: block;
    line-height: 2;
    padding:0px 21px 5px 10px;
    text-decoration: none;
}

.header .help-center-list a:hover{
    color: #900;
    background: #d9d9d9;
}
.header .myaccount-list a:hover{
    color: #900;
    background: #d9d9d9;
}
.header .myaccount a,.header .help-center a{
    display: inline-block;  
    height: 27px;
    text-decoration: none;
}
.header .js_down-app{
    width: 97.5px;
}
.header .phone-box{
    margin-left: 1px;
    position: absolute;
    z-index: 2;
}
.header .down-app{
    width: 158px;
    height: 158px;
    padding: 10px;
    background-color: #fff;
    border: 1px solid #ededed;
    border-radius: 10px;
    position: absolute;
    top: 19px;
    left: 50%;
    margin-left: -90px;
    display: none;
    z-index: 1;
}

.header .all-class{
    padding:10px 10px 12px ;
    border-bottom: 1px solid #ededed;
}
.header .js_all-class{
    width: 125.5px;
}
.header .all-class-list-box{
    width: 100%;
    background-color: #fff;
    position: absolute;
    top: 40px;
    left: 0;
    display: none;
    padding-bottom: 15px;
    border-bottom: 1px solid #ededed;
}
.header .all-class-list{
    width: 1200px;
    margin: 0 auto;
}
.header .all-class-list li{
    width: 240px;
    height: 70px;
    margin: 15px 60px 0 0;
    float: left;
}
.header .all-class-list h4 {
    border-bottom: 1px solid #d9d9d9;
    font-weight: bold;
}
.header .all-class-list h4 a{
    font-size: 14px;
    line-height:26px;
}
.header .all-class-list p a{
    line-height:22px;
    margin-right: 10px;
    float: left;
}
.header .more{
    float: right
}
.header .all-class-list a{
    text-decoration: none;
}
.header .all-class-list p a:hover{
    color: #c52d15;
}
.header .all-class-list h4:hover a{
    color: #c52d15;
}
.header .service{
    width: 140px;
    height: 30px;
    position: absolute;
    top: 20px;
    right: 0;
    background: url(../images/index/header_1407.png) -130px -135px no-repeat;
    display: none;
}
.header .call:hover .service{
    display: block;
}
.header .js_myaccount .active,.header .js_help-center .active{
    background: #fff;
    border: 1px solid #ededed;
}
.header .js_down-app .active{
    background: #fff; 
    border: 1px solid #ededed;
    border-bottom: 1px solid #fff;
}
.header .js_all-class .active{
    background: #fff; 
    border: 1px solid #ededed; 
    border-bottom: 1px solid #fff; 
    padding-bottom: 12px; 
}
/* ======================头部 结束*/

/* ======================搜索 */
.search{
    width: 100%;
    min-width: 1200px;
}
.search .search-content{
	width: 1200px;
	height: 100px;
	margin:0 auto;
	position: relative;
}
.search .logo{
    width: 75px;
    height: 75px;
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -37.5px;
    background: url(../images/index/logo.png) 0 0 no-repeat
}
.search .search-right{
	width: 1200px;
    height: 75px;
    margin-top:25px ;
    
}
.search .search-right-top{
	width: 1200px;
	height: 36px;
}
.search .search-right-top-left{
	height: 30px;
	float: left;
    padding: 3px;
    position: relative;
    left: 300px;
    background:  url(../images/index/header_1407.png) 0px -46px no-repeat;
    background-size: 540px 206px;
}
.search .text{
	width: 440px;
	height: 30px;
	border: none;
	outline: none;
	font-size: 14px;
	line-height: 35px;
	background-color:#fff;
	vertical-align: top;
    float: left;
    padding-left: 5px;
}

.search .btn{
	width: 85px;
	height: 30px;
	border:none;
	outline: none;
    background-color:transparent;
    margin-left: 2px;
	float: left;
}
.search .shopcard-box{
	width: 158px;
	height: 34px;
    position: absolute;
    top: 25px;
    right: 0;
    border: 1px solid #bdbdbb;
    background: url(../images/index/header_1407.png) 0px -86px no-repeat;
}
.search .shopcard-text{
    
    color: #666;
	position: absolute;
	top: 10px;
	left: 33px;
}
.search .shopcard-num{
    color: #900;
    font-weight: bold;
    font-size: 16px;
	position: absolute;
	top: 8px;
	right: 42px;

}

.search .hotkeys{
	width: 510px;
    height: 30px;
    position: absolute;
    top: 70px;
    left: 350px;
}
.search .hotkeys ul li{
	float: left;
}
.search .hotkeys li{
	margin:0 5px;
}
.search .hot{
    color: #c51010;
}
.search .fix-search{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 49px;
    background-color: rgba(255,255,255,0.9);
    padding-top: 10px;
    border-bottom: 1px solid #d5d5d5;
    box-shadow: 1px 1px 3px #d5d5d5;
    z-index: 10;
}
.search .search-box{
    width: 1200px;
    margin: 0 auto;
    height: 40px;
}
.search .fix-logo{
    background-image: url(../images/index/zjl_icon_002.png);
    width: 54px;
    height: 40px;
    float: left;
}
.search .search-list{
    position: absolute;
    top: 36px;
    left: 0;
    padding: 0;
    width:450px;
    border: solid 1px #dfdfdf;
    background: #fff;
    z-index: 5;
    box-shadow: 1px 1px 3px #dfdfdf;
}
.search .search-list li{
    padding: 3px 8px 4px 8px;
    line-height: 26px;
    cursor: pointer;
    margin: 0;
    display: block;
    clear: both;
    overflow: hidden;
    color: #666;
}
.search .search-list span{
    color: #222;
}
.search .search-list li:hover{
    background-color: #f3f3f3;
}
.search .search-list li:hover .fl{
    color: #c51010;
}

/* ======================搜索 结束 */

/*===================导航*/
.nav{
    width: 100%;
    min-width: 1200px;
    background-color: #a71b1b;
}
.nav .nav-content{
	width: 1200px;
	height: 40px;
	margin:0 auto;
    position: relative;
    z-index: 2;
}
.nav .nav-left,.nav .nav-right{
    float: left;
}
.nav .all-class{
    width: 190px;
    height: 40px;
    background: url(../images/index/sprite_category.png) 0px -16px no-repeat;
}
.nav .all-class >a{
    font-size: 14px;
    padding: 2px;
    margin-left: 42px;
    line-height: 39px;
    color: #fff;
    background-color: #b82424;
    font-weight: bold;
}
.nav .nav-right li{
    float: left;

}
.nav .nav-right li a{
    width: 100px;
    height: 40px;
    line-height: 40px;
    display: block;
    text-align: center;
    color: #fff;
    font-size: 15px;
    font-weight: bolder;
    text-decoration: none;
}
.nav .nav-right li a:hover{
    background-color: #6c0709
}
.nav .nav-left a{
    text-decoration: none;
}
.nav .nav-left .all-class:hover{
    background: url(../images/index/sprite_category.png) 0px -76px no-repeat;
}
.nav .nav-left .all-class:hover >a{
    background: #c52e15;
}
.nav .item{
    width: 190px;
    height: 470px;
    background-color: rgba(255,255,255,0.9);
    position: absolute;
    left: 0;
    top: 40px;
}
.nav .item>ul>li{
    width: 170px;
    height: 36px;
    line-height: 36px;
    padding: 0 5px 0 15px;
    font-size: 13px;
}
.nav .item i{
    float: left;
    width: 16px;
    height: 16px;
    margin:10px 10px 0 0;
    transform: rotate(0deg);
    transition:all .5s;
}
.nav .feature i{
    background: url(../images/index/sprite_category.png) -144px 0px no-repeat
}
.nav .beauty i{
    background: url(../images/index/sprite_category.png) -48px 0px no-repeat
}
.nav .home i{
    background: url(../images/index/sprite_category.png) -192px 0px no-repeat
}
.nav .kitchen i{
    background: url(../images/index/sprite_category.png) -176px 0px no-repeat
}
.nav .household i{
    background: url(../images/index/sprite_category.png) -32px 0px no-repeat
}
.nav .education i{
    background: url(../images/index/sprite_category.png) -128px 0px no-repeat
}
.nav .bags i{
    background: url(../images/index/sprite_category.png) -80px 0px no-repeat
}
.nav .phone i{
    background: url(../images/index/sprite_category.png) -0px 0px no-repeat
}
.nav .clothes i{
    background: url(../images/index/sprite_category.png) -64px 0px no-repeat
}
.nav .movement i{
    background: url(../images/index/sprite_category.png) -112px 0px no-repeat
}
.nav .computer i{
    background: url(../images/index/sprite_category.png) -16px 0px no-repeat
}
.nav .car i{
    background: url(../images/index/sprite_category.png) -272px -16px no-repeat
}
.nav .card i{
    background: url(../images/index/sprite_category.png) -256px 0px no-repeat
}
.nav .item li:hover i{
    transform: rotate(360deg);
    transition: all .5s;
}
.nav .item >ul>li>a:hover{
    text-decoration: underline;
    color: #c51010;
}
.nav .item >ul>li:hover {
    box-shadow: 1px 1px 3px #888;
    
    background-color: #fff;

}
.nav .item-box{
    width: 1010px;
    height: 470px;
    background-color: #fff;
    position: absolute;
    top: 0;
    left: 190px;
    display: none;
}
.nav .item-list-box{
    width: 700px;
    float: left;
}
.nav .item-class{
    float: left;
}
.nav .item-list-box ul{
    overflow: hidden;
}
.nav .item-list-box li{
    float: left;
    line-height: 22px;
    font-size: 12px;
    margin-right: 20px;
}
.nav .item-ad{
    width: 330px;
}
.nav .item>ul>li:hover .item-box {
    display: block;
}
.nav .item-class a{
    color: #333;
    font-size: 14px;
    font-weight: bold;
}
.nav .item-class{
   width: 100px;
   padding: 0 20px 0 10px;
   line-height: 22px;
   font-size: 12px;
}
.nav .item-list-box div{
    width: 100%;
    border-bottom: 1px solid #ebebeb;
    padding: 8px 0;
}
.nav .item-list-box li a:hover{
    color: #c51010;
    text-decoration: underline;
}
.nav .item-ad{
    width: 260px;
    height: 470px;
    padding: 10px 25px 0;
    background-color: #fff;
    float: left;
}
.nav .item-ad-top div{
    padding: 5px 10px;
    float: left;
}
.nav .item-ad-top img{
    width: 110px;
    height: 52px;
}

.nav .item-list-box-bottom{
    position: absolute;
    bottom: 0;
    left: 20px;
}
.nav .item-list-box-bottom a{
    float: left;
    margin: 0 30px 45px 0; 
}
.nav .last-list{
    width: 600px;
    float: left;
}
.nav .last{
    width: 380px;
    height: 400px;
    padding: 70px 0 0;
    background-color: #fff;
    float: left;
}
/*===================导航 结束*/

/*==================banner*/
.banner{
    width: 100%;
    background: #5322e6;
}

.banner .banner-content {
    width: 1200px;
    height: 470px; 
    margin: 0 auto;
    position: relative;
}
.banner .dot li {
    float: left;
    text-align: center;
    width: 15px;
    height: 15px;
    margin: 0 5px;
    border-radius: 100%;
    background-color: #fff;
}
.banner .dot .active{
    background-color:#c7181b;
}
.banner .dot {
    width: 150px;
    height: 15px;
    margin: 0 auto;
    position: absolute;
    bottom: 25px;
    right:280px;
    margin-left: -75px;
    z-index: 1;
}
.banner .banner-box {
    width: 1200px;
    height: 470px;
}

.banner .banner-box >div {
    width: 1200px;
    height: 470px;
    display: none;
}
.banner .banner-page>img{
    width: 1200px;
