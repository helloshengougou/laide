}
 
.banner .banner-page >div{
    width: 265px;
    height: 470px;
    position: absolute;
    right: 0;
    top: 0;
    /* display: none; */
}
.banner .banner-page >div>div{
    margin-top: 10px;
    width: 265px;
    height: 143px;
   background-color: #fff;
}
